    "@types/brotli": "^1.3.1",
    "@types/matter-js": "^0.17.5",
    "@types/node": "^16.7.1",
    "@types/offscreencanvas": "^2019.6.4",
    "@types/uuid": "^8.3.1",
    "brotli": "^1.3.2",
    "cross-env": "^7.0.3",
    "lerna": "^4.0.0",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.56.2",
    "rollup-plugin-jscc": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.0",
