    "vite": "^2.5.0",
    "workspaces-run": "^1.0.1"
  },
  "dependencies": {
    "@pixi/particle-emitter": "^5.0.2",
    "free-tex-packer-core": "^0.3.4",
    "matter-js": "^0.17.1",
    "pixi-spine": "^3.0.13",
    "pixi.js": "^6.1.2",
    "sass": "^1.43.4",
    "uuid": "^8.3.2"
  }
