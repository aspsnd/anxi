{
  "name": "anxi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "workspaces": [
    "bundles/*",
    "packages/*"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "cd ./examples && vite",
    "tex": "ts-node ./script/texTransfor.ts -P ./script/tsconfig.json",
